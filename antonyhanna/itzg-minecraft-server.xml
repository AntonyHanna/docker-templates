<?xml version="1.0" encoding="UTF-8"?>
<Container>
	<TemplateURL>https://raw.githubusercontent.com/AntonyHanna/docker-templates/master/antonyhanna/blyatmir-putin-bot.xml</TemplateURL>
	<Beta>True</Beta>
	<Category>GameServers</Category>
	<Date>2020/05/03</Date>
	<Name>minecraft-server</Name>
	<Overview>Explore your own unique world, survive the night, and create anything you can imagine!</Overview>
	<Project>https://github.com/itzg/docker-minecraft-server</Project>
	<Repository>itzg/docker-minecraft-server</Repository>
	<Registry>https://hub.docker.com/r/itzg/minecraft-server/</Registry>
	<Network>bridge</Network>
	<BindTime>True</BindTime>
	<Priveleged>False</Priveleged>
	<Icon>https://raw.githubusercontent.com/antonyhanna/docker-templates/master/antonyhanna/images/minecraft-server.png</Icon>
	<Networking>
		<Mode>bridge</Mode>
	</Networking>
	<Labels/>
	<Config Name="EULA" Target="EULA" Default="false" Mode="" Description="Minecraft Server End User License Agreement" Type="Variable" Display="always" Required="true" Mask="false"></Config>
	<Config Name="Version" Target="VERSION" Default="" Mode="" Description="Version of Minecraft Server to download, don't specify for latest" Type="" Display="always" Required="true" Mask="false"></Config>
	<Config Name="Server Port" Target="25565" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">25565</Config>
	<Config Name="Data" Target="/data" Default="!" Mode="/mnt/cache/appdata/minecraft-server" Description="Where to place your Minecraft Server files" Type="Path" Display="always" Required="!" Mask="false">/mnt/cache/appdata/minecraft-server</Config>
</Container>